
public class LabAssesment {
	public static void main(String args[]){
		isGoofy(15);
		
	}
	
	public static boolean isGoofy(int num){
		
		int number = num;
		int count = 0;
		System.out.println(number);
		while (number > 10){
			number = number/10;
			count += 1;
		}
		System.out.println(number);
		
		
		
		return true;
		
	}
	

}
