	}
	
	public static boolean isGoofy(int num){
		
		int number = num;
		int count = 0;
		System.out.println(number);
		while (number % 10 > 0){
			number = number/10;
			count += 1;
		}
		System.out.println(number);
		for (int i = 0; i<count; i++)
		
		
		return true;
		
	}
	

}
