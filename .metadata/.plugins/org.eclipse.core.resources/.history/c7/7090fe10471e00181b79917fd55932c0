
public class LabAssesment {
	public static void main(String args[]){
		isGoofy(25);
		
	}
	
	public static boolean isGoofy(int num){
		
		int number = num;
		int count = 0;
		int sum = 0;
		int arr [];
		System.out.println(number);
		while (number > 10){
			number = number/10;
			count += 1;
			for (int i = 0; i<count;i++){
				arr = new int[]{number};
				System.out.println(arr);
			}
		}
		System.out.println(number);
		
		
		
		return true;
		
	}
	

}
